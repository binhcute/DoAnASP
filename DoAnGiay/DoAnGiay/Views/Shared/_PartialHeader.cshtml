@model IEnumerable<DoAnGiay.Areas.Admin.Models.TypeShoeModel>
    <div class="header-top">
        <div class="wrap">
            <div class="logo">
                <a asp-action="Index" asp-controller="Home"><img src="~/user/images/logo.png" alt="" /></a>
            </div>
            <div class="cssmenu">
                <ul>
                    <li class="active"><a asp-action="Index" asp-controller="Register">Đăng Ký Ngay</a></li>
                    <li><a href="shop.html">Store Locator</a></li>
                    <li><a asp-action="Index" asp-controller="Login">Đăng Nhập</a></li>
                    <li><a href="checkout.html">CheckOut</a></li>
                </ul>
            </div>
            <ul class="icon2 sub-icon2 profile_img">
                <li>
                    <a class="active-icon c2" href="#"> </a>
                    <ul class="sub-icon2 list">
                        <li><h3>Products</h3><a href=""></a></li>
                        <li><p>Lorem ipsum dolor sit amet, consectetuer  <a href="">adipiscing elit, sed diam</a></p></li>
                    </ul>
                </li>
            </ul>
            <div class="clear"></div>
        </div>
    </div>

    <div class="header-bottom">
        <div class="wrap">
            <!-- start header menu -->
            <ul class="megamenu skyblue">
                <li><a class="color1" asp-controller="Home" asp-action="Index">Home</a></li>
                <li class="grid">
                    @foreach (var item in Model)
                    {
                    <a class="color2" asp-controller="TypeShoe" asp-action="Index" asp-route-id="@item.IdType">@Html.DisplayFor(modelItem => item.Name)</a>
                    }
                
            </ul>
            <div class="clear"></div>
        </div>
    </div>
